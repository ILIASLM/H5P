{
  "name": "srag/h5p",
  "description": "Add H5P contents in repository objects",
  "version": "2.7.1",
  "extra": {
    "ilias_plugin": {
      "id": "xhfp",
      "name": "H5P",
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999",
      "lucene_search": true,
      "slot": "Services/Repository/RepositoryObject"
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_PLUGIN",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "project",
  "keywords": [
    "h5p",
    "ilias",
    "object",
    "plugin",
    "repository"
  ],
  "homepage": "https://github.com/studer-raimann/H5P",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_PLH5P"
  },
  "require": {
    "php": ">=7.0",
    "h5p/h5p-core": "1.24.0",
    "h5p/h5p-editor": "1.24.1",
    "srag/activerecordconfig": ">=0.1.0",
    "srag/custominputguis": ">=0.1.0",
    "srag/devtools": ">=0.1.0",
    "srag/dic": ">=0.1.0",
    "srag/generateplugininfoshelper": ">=0.1.0",
    "srag/librariesnamespacechanger": ">=0.1.0",
    "srag/removeplugindataconfirm": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\H5P\\": "src/"
    },
    "classmap": [
      "classes/",
      "src/Action/class.H5PActionGUI.php"
    ],
    "files": [
      "vendor/srag/dic/src/PHPVersionChecker.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\LibrariesNamespaceChanger\\PHP72Backport::PHP72Backport",
      "srag\\GeneratePluginInfosHelper\\H5P\\GeneratePluginPhpAndXml::generatePluginPhpAndXml",
      "srag\\GeneratePluginInfosHelper\\H5P\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}
